{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://softeerorange.store/api/:path*"
    },
    {
      "source": "/events/create",
      "destination": "/events/create.html"
    },
    {
      "source": "/events/:path(:path((?!create(\\.html)?$).*)",
      "destination": "/events/[id].html"
    },
    {
      "source": "/comments/(.*)",
      "destination": "/comments/[id].html"
    }
  ],
  "cleanUrls": true
}
